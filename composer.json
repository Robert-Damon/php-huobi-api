{
  "name": "Robert-Damon/php-huobi-api",
  "description": "PHP Huobi API is an asynchronous PHP library for the Huobi API designed to be easy to use.",
  "keywords": ["Huobi","cryptocurrency","Rest"],
  "license": "MIT",
  "repositories": [ {
    "type": "vcs",
    "url": "https://github.com/Robert-Damon/php-huobi-api"
  }],
  "require": {
    "php": ">=7.0",
    "ext-curl": "*",
    "ratchet/pawl": "^0.3.0",
    "react/socket": "^1.0 || ^0.8 || ^0.7",
    "ratchet/rfc6455": "^0.2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~6",
    "codacy/coverage": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "minimum-stability": "stable"
  },
  "autoload": {
    "classmap": [
      "lib.php"
    ]
  }
}